<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css'/>
</head>
<body>
<header>
    <h1><%= title %></h1>
    <ul class="type" id="type">
        <li data-type="dot" class="selected">DOT</li>
        <li data-type="stripe" >STRIPE</li>
    </ul>
    <p>
        Volume <input id="volume" type="range" min="0" max="100"
                      value="30">
    </p>
</header>
<div class="container">
    <div class="left">
        <ul id="list">
            <% music.forEach(name =>{ %>
            <li title="<%= name %>"><%= name %></li>
            <% }) %>
        </ul>
    </div>
    <div class="right" id="box"></div>
</div>
<script src="/javascripts/MusicVisualizer.js"></script>
<script src="/javascripts/index.js"></script>

</body>
</html>
